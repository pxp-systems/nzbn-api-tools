  /users:
    get:
      tags:
        - 03. Users
      summary: NZBN_101_1 - Get the details of the currently logged in user
      description: >-
        Get the details of the currently logged in user (the only user resource
        the current user has authorisation to see)
      operationId: UsersGet
      produces:
        - application/json
      parameters:
        - in: header
          name: api-business-govt-nz-Request-Id
          description: >-
            Request id. Provide a UUID that is unique to this request.  This
            header is not required.
          type: string
      responses:
        '200':
          description: Success
          schema:
            $ref: '#/definitions/UserList'
          examples:
            application/json:
              users:
                - userId: string
                  status: string
                  acceptTerms: true
                  postalAddress:
                    uniqueIdentifier: string
                    startDate: string
                    endDate: string
                    careOf: string
                    address1: string
                    address2: string
                    address3: string
                    address4: string
                    postCode: string
                    countryCode: string
                    addressType: POSTAL
                    pafId: string
                  physicalAddress:
                    uniqueIdentifier: string
                    startDate: string
                    endDate: string
                    careOf: string
                    address1: string
                    address2: string
                    address3: string
                    address4: string
                    postCode: string
                    countryCode: string
                    addressType: POSTAL
                    pafId: string
                  phone:
                    areaCode: string
                    countryCode: string
                    number: string
                  fax:
                    areaCode: string
                    countryCode: string
                    number: string
                  mobile:
                    areaCode: string
                    countryCode: string
                    number: string
                  email: string
                  firstName: string
                  middleNames: string
                  lastName: string
                  fullName: string
          headers:
            If-None-Match:
              type: string
            api-business-govt-nz-Request-Id:
