  /entities/changes:
    get:
      tags:
        - 01. Entities
      summary: NZBN_001_2 - Search NZBN Entity Changes
      description: Search for NZBN entities based on change history.
      operationId: EntitiesChangesGet
      produces:
        - application/json
      parameters:
        - in: query
          name: change-event-type
          description: >
            A comma separated list of change events to match when searching the
            change history

            ALL,

            NEW,

            ENTITY,

            ENTITY_NAME,

            ENTITY_STATUS,

            ADDRESSES_REGISTERED,

            ADDRESSES_OFFICE,

            ADDRESSES_POSTAL,

            ADDRESSES_RECORDS,

            ADDRESSES_SHAREREGISTER,

            ADDRESSES_INVOICE,

            ADDRESSES_DELIVERY,

            ADDRESSES_SERVICE,

            COMPANYDETAILS_FRAFILINGMONTH,

            COMPANYDETAILS_ARFILINGMONTH,

            COMPANYDETAILS_CONSTITUTIONFILED,

            COMPANYDETAILS_COUNTRYOFORIGIN,

            COMPANYDETAILS_NZSXCODE,

            COMPANYDETAILS_ACNNUMBER,

            COMPANYDETAILS_ULTIMATEHOLDINGCOMPANY,

            COMPANYDETAILS_SHAREHOLDING,

            COMPANYDETAILS_INSOLVENCYDETAILS,

            COMPANYDETAILS_FILINGS, 

            COMPANYDETAILS_REMOVALCOMMENCED,  

            COMPANYDETAILS_ANNUALRETURNS,

            COMPANYDETAILS_FINANCIALSTATEMENTS,          

            ROLES_DIRECTOR,

            ROLES_SOLETRADER,

            ROLES_TRUSTEE_INDIVIDUAL,

            ROLES_PARTNER_INDIVIDUAL,

            ROLES_TRUSTEE_OTHER,

            ROLES_PARTNER_OTHER,

            ROLES_PARTNER_GENERAL_PARTNER,

            ROLES_PERSONAUTHORISEDFORSERVICE,

            PRIVACYSETTINGS,

            TRADINGNAMES,

            WEBSITES,

            PHONENUMBERS,

            EMAILADDRESSES,

            GSTNUMBER,

            TRADINGAREAS,

            AUSTRALIANBUSINESSNUMBER,

            INDUSTRYCLASSIFICATIONS,

            BUSINESSETHNICITYIDENTIFIER
          required: true
          type: string
        - in: query
          name: entity-type
          description: >-
            Entity type search filter element (one or more of Trading_Trust,
            Sole_Trader, Y, Z, GovtCentral, GovtEdu, GovtLocal, GovtOther, LTD,
            ULTD, COOP,ASIC, NON_ASIC
          type: string
        - in: query
          name: date-time-from
          description: Start date for change history search (format YYYY-MM-DDThh:mm:ss)
          type: string
        - in: query
          name: date-time-to
          description: End date for change history search (format YYYY-MM-DDThh:mm:ss)
          type: string
        - in: query
          name: page
          description: >-
            Format - int32. Number of search records page (for pagination), zero
            indexed
          type: integer
        - in: query
          name: page-size
          description: >-
            Format - int32. Number of search records to be returned (for
            pagination)
          type: integer
        - in: query
          name: sort-by
          description: Parameter by which to sort the search results.
          type: string
          default: NZBN
          enum:
            - NZBN
            - ENTITY_NAME
            - ENTITY_STATUS
            - ENTITY_TYPE_CODE
        - in: query
          name: sort-order
          description: >-
            API Consumer can specify "ASC" or "DESC" to sort by the attribute
            specified in the sortBy parameter.   ASC will sort in ascending
            order, DESC will sort in descending order.
          type: string
          default: ASC
          enum:
            - ASC
            - DESC
        - in: header
          name: api-business-govt-nz-Request-Id
          description: >-
            Request id. Provide a UUID that is unique to this request.  This
            header is not required.
          type: string
      responses:
        '200':
          description: >-
            Returns a search result array of entities. Results are sorted by
            relevance.
          schema:
            $ref: '#/definitions/SearchEntityChangeResponse'
          examples:
            application/json:
              pageSize: 0
              page: 0
              totalItems: 0
              sortBy: string
              sortOrder: string
              items:
                - nzbn: string
                  entityName: string
                  changeDateTime: string
                  items:
                    - changeEventType: string
                      newValue: string
                      resourceLink: string
                      link:
                        rel: string
                        href: string
                        methods:
                          - string
              links:
                - rel: string
                  href: string
                  methods:
                    - string
          headers:
            api-business-govt-nz-Request-Id:
              type: string
            api-business-govt-nz-Correlation-Id:
              type: string
        '400':
          description: >-
            Request failed validation. Appropriate headers not provided or
            request did not validate or is not supported.
          schema:
            $ref: '#/definitions/ErrorResponse'
          examples:
            application/json:
              status: string
