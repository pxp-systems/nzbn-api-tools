  OrganisationPartPurchase:
    title: Purchase
    description: The Organisation Part purchase details
    required:
      - purchaserName
      - purchaserEmailAddress
      - purchaserPhoneNumber
      - purchaseRedirectUrl
    type: object
    properties:
      purchaseId:
        description: The id assigned to the purchase
        type: string
      purchaserName:
        description: >-
          First and last name (surname) of the person purchasing the OPN on
          behalf of the associated NZBN entity
        type: string
      purchaserEmailAddress:
        description: >-
          Email address for the person purchasing the OPN on behalf of the
          associated NZBN entity
        type: string
      purchaserPhoneNumber:
        description: >-
          Phone number for the person purchasing the OPN on behalf of the
          associated NZBN entity
        type: string
      purchaseRedirectUrl:
        description: >-
          The callback url for GS1 to redirect control back to the API Consumer 
          Application after purchase process is completed.
        type: string
      purchaseAmount:
        description: The purchase amount for the OPN
        type: number
      purchaseStatus:
        $ref: '#/definitions/PurchaseStatusType'
      paymentTransactionId:
        description: Payment provider transaction id
        type: string
      paymentOrderUrl:
        description: The form url issued by the provider to submit the payment
        type: string
      paymentOrderTime:
        format: date-time
        description: The date this payment transaction was created.
        type: string
      paymentNumber:
        description: The unique bank reference assigned to the transaction
        type: string
      paymentResponseCode:
        description: The acquirer response code for the transaction
        type: string
      paymentReceiptNumber:
        description: virtual terminal counter for the transaction (not unique)
        type: string
      issuedOpn:
        description: The OPN issued for an approved purchase
        type: string
  PurchaseStatusType:
    title: Purchase Status Type
