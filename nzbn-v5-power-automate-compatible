        $ref: '#/definitions/RolePerson'
      roleAddress:
        type: array
        items:
          $ref: '#/definitions/Address'
      roleAsicAddress:
        $ref: '#/definitions/Address'
      uniqueIdentifier:
        description: A code to allow updating of this record.
        type: string
