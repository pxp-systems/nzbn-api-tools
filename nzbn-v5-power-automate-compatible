  /authorities:
    get:
      tags:
        - 06. Authorities
      summary: NZBN_103_1 - Get Authority
      description: >-
        This will return authorities for given nzbn, user, organisation and can
        filter on status, e.g. PENDING.  Can specify one of nzbn, user-id,
        organisation-id, approver-user-id. Can further refine by specifying
        authority-status.
      operationId: AuthoritiesGet
      produces:
        - application/json
      parameters:
        - in: query
          name: nzbn
          description: >-
            Return all authorities relevant to this entity.  The nzbn can be
            provided along with other parameters to test if a given user,
            organisation, or approver has an authority relevant to this entity
          type: string
        - in: query
          name: user-id
          description: >-
            return all authorities relevant to this user - whether allocated
            personally or allocated to an organisation this user is a member of
          type: string
        - in: query
          name: organisation-id
          description: Returns all authorities relevant to this Organisation
          type: string
        - in: query
          name: historic
          description: >-
            if False (default), return only authorities that are current (not
            historic); granted (not revoked or pending); and for current
            entities (not removed ones). If True, return all current, pending,
            revoked and historic authorities.
          type: boolean
        - in: query
          name: page
          description: >-
            Format - int32. Number of search records page (for pagination), zero
            indexed
          type: integer
        - in: query
          name: page-size
          description: >-
            Format - int32. Number of search records to be returned (for
            pagination)
          type: integer
        - in: query
          name: sort-by
          description: Name of the field to sort the response by
          type: string
        - in: query
          name: sortorder
          description: Whether to sort the response in ascending or descending order
          type: string
          enum:
            - ASC
            - DESC
        - in: header
          name: api-business-govt-nz-Request-Id
          description: >-
            Request id. Provide a UUID that is unique to this request.  This
            header is not required.
          type: string
      responses:
        '200':
          description: Success
          schema:
            $ref: '#/definitions/AuthoritiesSearchResponse'
          examples:
            application/json:
              pageSize: 0
              page: 0
              totalItems: 0
              sortBy: string
              sortOrder: string
              items:
                - authorityId: string
                  nzbn: string
                  entityName: string
                  entityStatusCode: string
                  entityTypeCode: string
                  userId: string
                  organisationId: string
                  partyName: string
                  authorityStartDate: string
                  AuthorityStatus: PENDING
                  AuthorityType: DELEGATED
                  approvalType: REGISTRAR_LETTER
                  supportingInformation:
                    - metadataId: string
                      metadataType: string
                      Name: string
                      DateOfBirth: string
                      Number: string
                      Version: string
                      ExpiryDate: string
                  documents:
                    - documentId: string
                      documentType: string
                      url:
                        rel: string
                        href: string
                        methods:
                          - string
                      document:
                        rel: string
                        href: string
                        methods:
                          - string
                  reason: string
                  comment: string
              links:
                - rel: string
                  href: string
                  methods:
                    - string
          headers:
            api-business-govt-nz-Request-Id:
              type: string
            api-business-govt-nz-Correlation-Id:
              type: string
        '400':
          description: >-
            Request failed validation. Appropriate headers not provided or
            request did not validate or is not supported.
          schema:
            $ref: '#/definitions/ErrorResponse'
          examples:
            application/json:
              status: string
              errorDescription: string
              errorCode: string
              list:
                - field: string
                  message: string
          headers:
            api-business-govt-nz-Request-Id:
              type: string
            api-business-govt-nz-Correlation-Id:
              type: string
