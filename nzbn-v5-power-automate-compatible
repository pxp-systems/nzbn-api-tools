  /watchlists:
    get:
      tags:
        - 08. Watchlists
      summary: NZBN_008 - Search Existing watchlists
      description: >-
        Searches existing watchlists; returns all watchlists that can be managed
        by the current user, if no parameters are passed.
      operationId: WatchlistsGet
      produces:
        - application/json
      parameters:
        - in: query
          name: name
          description: >-
            Optional, allows entry of a text string.  The search will attempt to
            match on watchlists where watchlist name contains the input string,
            for the currently authenticated user
          type: string
        - in: query
          name: organisation-id
          description: >-
            Optional, If blank this is a personal watchlist. If completed this
            is an organisation's watchlist and can be managed by any
            administrator of this organisation.  If entered; must be a valid
            Organisation Id for the NZBN system and one that the user is a
            member of. The search will return records only where an exact match
            is found.
          type: string
        - in: query
          name: page
          description: >-
            Format - int32. Number of search records page (for pagination), zero
            indexed
          type: integer
        - in: query
          name: page-size
          description: >-
            Format - int32. Number of search records to be returned (for
            pagination)
          type: integer
        - in: query
          name: sort-by
          description: Name of the field to sort the response by
          type: string
        - in: query
          name: sortorder
          description: Whether to sort the response in ascending or descending order
          type: string
          enum:
            - ASC
            - DESC
        - in: query
          name: change-event-types
          description: >-
            A change event type to match when receiving the change history in a
            notification. One or more change event type(s) to match with when
            specifying conditions for notifications in "Create New Watchlist". 
            List of comma separated values.
          type: string
        - in: header
          name: api-business-govt-nz-Request-Id
          description: >-
            Request id. Provide a UUID that is unique to this request.  This
            header is not required.
          type: string
      responses:
        '200':
          description: Success
          schema:
            $ref: '#/definitions/WatchlistSearchResponse'
          examples:
            application/json:
              pageSize: 0
              page: 0
              totalItems: 0
              sortBy: string
              sortOrder: string
              items:
                - watchlistId: 0
                  name: string
                  userId: string
                  fromDate: string
                  toDate: string
                  organisationId: string
                  channel: EMAIL
                  frequency: ONEVENT
                  wildcard: ALL
                  changeEventTypes: string
                  pushEndpoint: string
                  authorizationToken: string
                  adminEmailAddress: string
                  watchlistEmailAddresses: string
                  lastSuccessfulChangeNoticeId: 0
                  suspended: true
                  links:
                    - rel: string
                      href: string
                      methods:
                        - string
                  nzbns:
                    - string
              links:
                - rel: string
                  href: string
                  methods:
                    - string
          headers:
            api-business-govt-nz-Request-Id:
              type: string
            api-business-govt-nz-Correlation-Id:
              type: string
        '400':
          description: >-
            Request failed validation. Appropriate headers not provided or
            request did not validate or is not supported.
          schema:
