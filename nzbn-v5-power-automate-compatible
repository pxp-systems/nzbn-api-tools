  PrivacySettings:
    title: PrivacySettings
    description: >-
      A series of flags that indicate whether a business entity authority has
      elected to make various attributes public.  Only available when an entity
      is retrieved by a user who is logged in and has authority over the entity.
    type: object
    properties:
      namePrivateInformation:
        description: True if the name is to be private for this business entity.
        type: boolean
      tradingNamePrivateInformation:
        description: >-
          True if all tradingNames are be private for this business entity.  If
          not true all provided trading names will be seen.
        type: boolean
      businessClassificationPrivateInformation:
        description: >-
          True if all businessClassifications are to be private for this
          business entity. If not true all provided classifications will be
          seen.
        type: boolean
      phonePrivateInformation:
        description: >-
          True if all phones are to be private for this business entity.  If not
          true all provided phones will be seen.
        type: boolean
      emailPrivateInformation:
        description: >-
          True if all email addresses are to be private for this business
          entity.  If not true all provided email addresses will be seen.
        type: boolean
      partnersPrivateInformation:
        description: >-
          True if all partners are to be private for this business entity.  If
          not true all provided values will be seen.
        type: boolean
      trusteesPrivateInformation:
        description: True if the trustees is to be private for this business entity.
        type: boolean
      publicSuggestChangesYn:
        description: >-
          True if the authority of the entity permits public submissions of
          changes to primary business data.
        type: boolean
      abnPrivateInformation:
        description: True if the ABN is to be private for this business entity.
        type: boolean
      gstNumberPrivateInformation:
        description: True if the GST NUmber is to be private for this business entity.
        type: boolean
      paymentBankAccountNumbersPrivateInformation:
        description: >-
          True if the payment bank account numbers are to be private for this
          business entity.
        type: boolean
      registeredAddressPrivateInformation:
        description: >-
          True if the registeredAddress is to be private for this business
          entity.
        type: boolean
      postalAddressPrivateInformation:
        description: True if the postalAddress is to be private for this business entity.
        type: boolean
      serviceAddressPrivateInformation:
        description: True if the service address is to be private for this business entity.
        type: boolean
      invoiceAddressPrivateInformation:
        description: True if all invoiceaddress is to be private for this business entity.
        type: boolean
      deliveryAddressPrivateInformation:
        description: >-
          True if all delivery address is to be private for this business
          entity.
        type: boolean
      officeAddressPrivateInformation:
        description: True if all office address is to be private for this business entity.
        type: boolean
      businessEthnicityPrivateInformation:
        description: >-
          True if all business ethnicity identifier information is to be private
          for this business entity.
        type: boolean
  Roles:
    type: array
    items:
      $ref: '#/definitions/Role'
  Role:
    title: Role
    description: >-
      A person or other entity that is associated with this business entity -
      for instance as a director, officer or partner.  Role Type depends on the
      role types relevant to the type of the business entity. Only Companies,
      Sole Traders, Partnerships and Trading Trusts will return roles.
    type: object
    properties:
      roleType:
        description: >-
          The relationship between this person or entity and the business
          entity. Can be one of Director, Person Authorised for Service, Sole
          Trader, Partner, Trustee, Officer. For sole traders, there will always
          be a Sole Trader role which records the current First, Middle and Last
          Names of the person.
        type: string
      roleStatus:
        type: string
      startDate:
        format: date-time
        description: The date this relationship applies from.
        type: string
      endDate:
        format: date-time
        description: The date this relationship applies to.
        type: string
      asicDirectorshipYn:
        description: Is this an ASIC Directorship
        type: boolean
      asicName:
        type: string
      acn:
        type: string
      roleEntity:
        $ref: '#/definitions/RoleEntity'
