          $ref: '#/definitions/ErrorField'
  ErrorField:
    title: ErrorField
    type: object
    properties:
      field:
        type: string
      message:
        type: string
  FullEntity:
    title: FullEntity
    type: object
    properties:
      entityStatusCode:
        description: >-
          A code that identifies the business entity's current trading status,
          during formation, operation, and voluntary or involuntary winding up. 
          A full list is available on the API explorer website. Not required for
          POST.
        type: string
      entityName:
        description: >-
          The current registered name of the business entity, which may differ
          from its trading name(s) and historic entity names.  For sole traders
          the entity name is a concatenation of First, Middle and Last Names
          recorded in the sole trader role entry. Not required for POST.
        type: string
      nzbn:
        description: >-
          The 13 digit unique identifier of a New Zealand business entity. Not
          required for POST.
        type: string
      entityTypeCode:
        description: >-
          Entity type code. One of LTD, ULTD, COOP, ASIC, NON_ASIC, Sole_Trader,
          Partnership, Trading_Trust, B, I, D, F, N, S, T, Y, Z, GovtCentral,
          GovtLocal, GovtEdu, GovtOther
        type: string
      entityTypeDescription:
        description: >-
          One of NZ Limited Company, NZ Unlimited Company, NZ Co-operative
          Company, Overseas ASIC Company, Overseas Non-ASIC Company, Sole
          Trader, Partnership, Trust, Building Society, Incorporated Society,
          Credit Union , Friendly Society, Industrial & Provident Society,
          Special Body, Charitable Trust, Limited Partnership (NZ), Limited
          Partnership (Overseas), Central Government, Local Government,
          Education, Other Public Entity
        type: string
      entityStatusDescription:
        description: >-
          The description of the business entity's trading status, e.g. showing
          registered or in voluntary or involuntary winding up. Not required for
          POST.
        type: string
      registrationDate:
        format: date-time
        description: >-
          Records the date that the entity was registered. For entities that
          must register. For entities that must be registered on an entity
          register, this is the date the entity begins to exist. For other
          entities that may choose to register, this is the date the entity was
          recorded in the register. For these entities, the registered date may
          be the same as or after the start date. Optional for POST, defaults to
          current date.
        type: string
      sourceRegister:
        description: >-
          Register where entity detail is mastered. One of COMPANY, Sole_Trader,
          Partnership, Trading_Trust,  B, I, D, F, N, S, T, Y, Z, GovtCentral,
          GovtLocal, GovtEdu, GovtOther
        type: string
      sourceRegisterUniqueIdentifier:
        description: >-
          Entity identifier held in register for example Company Number. Not
          required for POST.
        type: string
      lastUpdatedDate:
        format: date-time
        description: The last time the entity was updated
        type: string
      australianBusinessNumber:
        description: >-
          Applicable to Companies and Overseas Companies domiciled in Australia.
          ABN is a unique eleven digit number that identifies a business or
          organisation that is operating in Australia. It is not compulsory in
          Australia to have an ABN and not everyone is entitled to one.
        type: string
      emailAddresses:
        $ref: '#/definitions/EmailAddresses'
      addresses:
        $ref: '#/definitions/Addresses'
      industryClassifications:
        $ref: '#/definitions/IndustryClassifications'
      phoneNumbers:
        $ref: '#/definitions/PhoneNumbers'
      websites:
        $ref: '#/definitions/Websites'
      tradingNames:
        $ref: '#/definitions/TradingNames'
      privacySettings:
        $ref: '#/definitions/PrivacySettings'
      company-details:
        $ref: '#/definitions/Company'
      non-company-details:
        $ref: '#/definitions/NonCompany'
      roles:
        $ref: '#/definitions/Roles'
      tradingAreas:
        $ref: '#/definitions/TradingAreas'
      gstNumbers:
        $ref: '#/definitions/GstNumbers'
      supporting-information:
        $ref: '#/definitions/SupportingInformation'
      hibernationStatusCode:
        type: string
      hibernationStatusDescription:
        type: string
      businessEthnicityIdentifiers:
        $ref: '#/definitions/BusinessEthnicityIdentifiers'
