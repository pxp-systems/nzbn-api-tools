      documentId:
        description: >-
          the identifier for this document, returned when uploaded using POST
          /documents.  Document must have been uploaded previously.
        type: string
      documentType:
        description: >-
          A code to identify what the document contains (eg drivers licence,
          trust deed etc).
        type: string
      url:
        $ref: '#/definitions/Link'
      document:
        $ref: '#/definitions/Link'
