  Address:
    title: Address
    required:
      - uniqueIdentifier
      - startDate
    type: object
    properties:
      uniqueIdentifier:
        description: A code to allow updating of this record.
        type: string
      startDate:
        format: date-time
        description: The date this address applies from. May be future dated
        type: string
      endDate:
        format: date-time
        description: >-
          The date this address applies to. Will be blank for current
          addresses.  We consider the latest address to be the current one, even
          if it is future dated
        type: string
      careOf:
        type: string
      address1:
        type: string
      address2:
        type: string
      address3:
        type: string
      address4:
        type: string
      postCode:
        description: A New Zealand or overseas postcode or zip code.
        type: string
      countryCode:
        description: ISO 2-character country code, eg NZ, GB, AU.
        type: string
      addressType:
        $ref: '#/definitions/AddressType'
      pafId:
        type: string
  AddressType:
    title: AddressType
    description: >-
      One of [postal address, registered address, address for service, office
      address, delivery address, invoice address, records address, shareholder
      address.
    enum:
      - POSTAL
      - REGISTERED
      - SERVICE
      - OFFICE
      - DELIVERY
      - INVOICE
      - RECORDS
      - SHAREREGISTER
      - ALL
    type: string
    example: POSTAL
  PhoneNumber:
    title: PhoneNumber
    type: object
    properties:
      uniqueIdentifier:
        description: A code to allow updating of this record
