    title: Authority
    description: >-
      A specific authorisation - for a user or an organisation, over an NZBN
      entity
    type: object
    properties:
      authorityId:
        description: the identifier for this authority resource. Blank for POST.
        type: string
      nzbn:
        description: >-
          the entity this authority is for.  Not required in a POST if the IR
          number is supplied (in supporting information) and entity type is Sole
          Trader, Partnership or Trust.
        type: string
      entityName:
        description: >-
          the name of the entity this authority is for (for populating results
          grids in responses).  Ignored in requests.
        type: string
      entityStatusCode:
        description: >-
          The code that identifies the business entity's current trading
          status.  A full list is available on the API explorer website. Ignored
          in requests.
        type: string
      entityTypeCode:
        description: >-
          the type of entity this authority is for (for populating results
          grids).  Ignored in requests.
        type: string
      userId:
        description: >-
          the user that holds (or is requesting/claiming/being granted) this
          authority (if a personal authority)
        type: string
      organisationId:
        description: >-
          the organisation that holds (or is requesting/claiming/being granted)
          this authority (if an organisational authority)
        type: string
      partyName:
        description: >-
          User name or organisation name, depending on whether authority is an
          individual or an organisation
        type: string
      authorityStartDate:
        format: date-time
        description: When the authority took effect
        type: string
      AuthorityStatus:
        $ref: '#/definitions/AuthorityStatus'
      AuthorityType:
        $ref: '#/definitions/AuthorityType'
      approvalType:
        $ref: '#/definitions/ApprovalType'
      supportingInformation:
        description: >-
          Used in POST or PUT request.  Information associated with this
          creation or update, such as proof of identity or proof of
          authorisation.
        type: array
        items:
          $ref: '#/definitions/Metadata'
      documents:
        description: >-
          Used in POST or PUT request.  Information associated with this
          creation or update, such as proof of identity or proof of letter of
          authorisation.
        type: array
        items:
          $ref: '#/definitions/Document'
      reason:
        description: >-
          Textual notes about why the last status change was made. Mandatory if
          authorityStatus is REJECTED. Must be between 10 and 500 chars. N/A for
          other statuses.
        type: string
      comment:
        description: >-
          Textual notes about this authority. Mandatory if authorityStatus is
          REJECTED or REVOKED. Must be between 10 and 500 chars. N/A for other
          statuses.
        type: string
