    get:
      tags:
        - 10. Find Organisation Parts
      summary: OP001 - Find Organisation Parts
      description: >-
        Find Organisation Parts allows the user to find organisation parts that
        match the specified parameters.  The response will return a collection
        containing all of the details for each of the matching organisation
        parts, if any.
      operationId: EntitiesOrganisationPartsFind
      produces:
        - application/json
      parameters:
        - in: query
          name: search-term
          description: The text string to search for.
          required: true
          type: string
        - in: query
          name: function
          description: One of (FUNCTION, PHYSICAL LOCATION, DIGITAL LOCATION).
          type: string
          enum:
            - FUNCTION
            - PHYSICAL_LOCATION
            - DIGITAL_LOCATION
        - in: query
          name: parent-nzbn
          description: >-
            The NZBN for the legal entity the organisation part belongs to/is
            part of.
          type: string
        - in: query
          name: parent-nzbn-name
          description: >-
            The name of the legal entity the organisation part belongs to/is
            part of.
          type: string
        - in: query
          name: organisation-part-status
          description: One of ACTIVE, INACTIVE. If not supplied will return all.
          type: string
          enum:
            - ACTIVE
            - INACTIVE
        - in: query
          name: page
          description: Format - int32. Number of search records page (for pagination)
          type: integer
        - in: query
          name: page-size
          description: >-
            Format - int32. Number of search records to be returned (for
            pagination)
          type: integer
        - in: header
          name: api-business-govt-nz-Request-Id
          description: >-
            Request id. Provide a UUID that is unique to this request.  This
            header is not required.
          type: string
        - in: header
          name: If-None-Match
          description: If-None-Match. Takes an ETag - optional for GET
          type: string
      responses:
        '200':
          description: Success
