swagger: '2.0'
info:
  title: NZBN
  description: >
    The New Zealand Business Number (NZBN) API provides a range of operations
    for getting information about a New Zealand business, updating details,
    managing watchlists, and managing business authorities. There is no fee for
    using this API.


    See [here](/api/nzbn) for additional information on how to use the API.
  version: v5
host: api.business.govt.nz
basePath: /gateway/nzbn/v5
schemes:
  - https
consumes: []
produces: []
paths:
  /entities:
    get:
      tags:
        - 01. Entities
      summary: NZBN_001_1 - Search the NZBN directory by name (text search)
      description: Search for an entity by name.
      operationId: EntitiesGet
      produces:
        - application/json
      parameters:
        - in: query
          name: search-term
          description: >-
            The text string to search for. Free-text search searches across
            current and past entity names, current and past trading names, NZBN
            and legacy numbers (eg company number).
          required: true
          type: string
        - in: query
          name: entity-status
          description: >-
            Entity status search filter element (one or more of Registered,
            VoluntaryAdministration, InReceivership, InLiquidation,
            InStatutoryAdministration,Inactive,RemovedClosed)
          type: string
        - in: query
          name: entity-type
          description: >-
            Entity type search filter element (one or more of
            NZCompany,OverseasCompany, SoleTrader, Partnership, Trust,
            BuildingSociety, CharitableTrust, CreditUnion, FriendlySociety,
            IncorporatedSociety, IndustrialAndProvidentSociety,
            LimitedPartnershipNz, LimitedPartnershipOverseas, SpecialBodies,
            SpecialBody, Trading_Trust, Sole_Trader, B, I, D, F, N, S, T, Y, Z,
            GovtCentral, GovtEdu, GovtLocal, GovtOther, LTD, ULTD, COOP,ASIC,
            NON_ASIC
          type: string
        - in: query
          name: industry-code
          description: Industry code search filter element
          type: string
        - in: query
          name: page
          description: >-
            Format - int32. Number of search records page (for pagination), zero
            indexed
          type: integer
        - in: query
          name: page-size
          description: >-
            Format - int32. Number of search records to be returned (for
            pagination)
          type: integer
        - in: header
          name: api-business-govt-nz-Request-Id
          description: >-
            Request id. Provide a UUID that is unique to this request.  This
            header is not required.
          type: string
      responses:
        '200':
          description: >-
            Returns a search result array of entities. Results are sorted by
            relevance.
          schema:
            $ref: '#/definitions/SearchEntityResponse'
          examples:
            application/json:
              pageSize: 0
              page: 0
              totalItems: 0
              sortBy: string
              sortOrder: string
              items:
                - entityStatusCode: string
                  entityName: string
                  nzbn: string
                  entityTypeCode: string
                  entityTypeDescription: string
                  entityStatusDescription: string
                  tradingNames:
                    - uniqueIdentifier: string
                      name: string
                      startDate: string
                      endDate: string
                  classifications:
                    - uniqueIdentifier: string
                      classificationCode: string
                      classificationDescription: string
                  previousEntityNames:
                    - string
                  registrationDate: string
                  sourceRegisterUniqueId: string
                  links:
                    - rel: string
                      href: string
                      methods:
                        - string
              links:
                - rel: string
                  href: string
                  methods:
                    - string
          headers:
            api-business-govt-nz-Request-Id:
              type: string
            api-business-govt-nz-Correlation-Id:
              type: string
        '400':
          description: >-
            Request failed validation. Appropriate headers not provided or
            request did not validate or is not supported.
          schema:
            $ref: '#/definitions/ErrorResponse'
          examples:
            application/json:
              status: string
              errorDescription: string
              errorCode: string
              list:
                - field: string
                  message: string
          headers:
            api-business-govt-nz-Request-Id:
              type: string
            api-business-govt-nz-Correlation-Id:
              type: string
        '401':
          description: >-
            Permission Denied, need to be authenticated to access or modify this
            resource.
          schema:
            $ref: '#/definitions/ErrorResponse'
          examples:
            application/json:
              status: string
              errorDescription: string
              errorCode: string
              list:
                - field: string
                  message: string
          headers:
            api-business-govt-nz-Request-Id:
              type: string
            api-business-govt-nz-Correlation-Id:
              type: string
        '403':
          description: >-
            Permission Denied, need to be authorised to access or modify this
            resource.
          schema:
            $ref: '#/definitions/ErrorResponse'
          examples:
            application/json:
              status: string
              errorDescription: string
              errorCode: string
              list:
                - field: string
                  message: string
          headers:
            api-business-govt-nz-Request-Id:
              type: string
            api-business-govt-nz-Correlation-Id:
              type: string
        '500':
          description: An internal error has ocurred.
          schema:
            $ref: '#/definitions/ErrorResponse'
          examples:
            application/json:
              status: string
              errorDescription: string
              errorCode: string
              list:
                - field: string
                  message: string
          headers:
            api-business-govt-nz-Request-Id:
              type: string
            api-business-govt-nz-Correlation-Id:
              type: string
