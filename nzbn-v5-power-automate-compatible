          name: api-business-govt-nz-Request-Id
          description: >-
            Request id. Provide a UUID that is unique to this request.  This
            header is not required.
          type: string
        - in: header
          name: If-None-Match
          description: If-None-Match. Takes an ETag - optional for GET
          type: string
        - in: body
          name: body
          description: NZBNs to be added
          schema:
            $ref: '#/definitions/ShareNzbns'
