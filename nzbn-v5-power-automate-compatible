  Website:
    title: Website
    type: object
    properties:
      uniqueIdentifier:
        description: A code to allow updating of this record.
        type: string
      url:
        description: The address of a website related to this business entity.
        type: string
      websitePurpose:
        description: The purpose of this website
        type: string
      startDate:
        format: date-time
        description: The date this website applies from.
        type: string
  Metadata:
    title: Metadata
    description: >-
      data on a document or other identifying material (such as a letter of
      authorisation, passport or IRD Number). Only the attributes relevant to
      information shown on the document should be entered.
    type: object
    properties:
      metadataId:
        description: Identifier of the metadata object.  Will be blank in requests.
        type: string
      metadataType:
        description: >-
          One of the following:

          * LETTER_OF_AUTH - Letter of Authorisation

          * DRIVERS_LICENCE - Drivers Licence

          * PASSPORT - Passport

          * IRD_NUMBER - IRD Number 

          * OTHER - other document to support request/claim

          metadataType is not an enum, as these type codes will be added to over
          time.  For Sole Traders, Partnerships and Trusts - a metadata object
          with metadataType = IRD_NUMBER is required.
        type: string
      Name:
        description: The person or business entity name, exactly as shown on the document
        type: string
      DateOfBirth:
        format: date-time
        description: The person's date of birth, exactly as shown on the document
        type: string
      Number:
        description: The document number (eg passport number or Drivers Licence number)
        type: string
      Version:
        description: The document version (specifically for Drivers Licence)
        type: string
      ExpiryDate:
        format: date-time
        description: the document expiry date, exactly as shown on the document
        type: string
  Documents:
    type: array
    items:
      $ref: '#/definitions/Document'
  Document:
    title: Document
    description: >-
      Reference to a file containing a scanned document, that was already
      uploaded using POST /documents .
    type: object
